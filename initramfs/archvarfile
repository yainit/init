# name of architecture, must be identic with that in the kernel Makefile 
ARCH="i386"
# kernel version without extents  
ARCH_KERNEL_VERSION="3.10.17.yainitrt11"
# A unique string for INIT versioning
ARCH_IDSTRING="yainit-$ARCH-20130810120001"
# all distributions with minor version number you can boot in by default without customizing 
ARCH_DEFAULT_DISTRO_COMPAT="opensuse-12.1 fedora-17 slackware-14"
#Prefix for some filenames: exs: spupsave.2fs, spup-030.sfs
ARCH_SYSTEMFILE_PREFIX=""
ARCH_SYSTEM_VERSION=""
ARCH_SYSTEMFILE_SFS='$ARCH_SYSTEMFILE_PREFIX-$ARCH_SYSTEM_VERSION.sfs'
ARCH_ZDRVSFS="$ARCH_SYSTEMFILE_PREFIX-$ARCH_KERNEL_VERSION-xz.sfs"
ARCH_ZDRVSFS_PREFIX="$ARCH_SYSTEMFILE_PREFIX"
ARCH_BROWSER_VERSION='17.0.8esr'
ARCH_BROWSER_NAME='firefox'
#
MODULE_WHITELIST="k10temp it87 fam15h_power gpio-fan lzo mperf pci_hotplug button video processor"
#
MODULE_BLACKLIST="kvm kvm_amd udl drm_usb"
#
#
export ARCH ARCH_KERNEL_VERSION ARCH_IDSTRING ARCH_DEFAULT_DISTRO_COMPAT ARCH_SYSTEMFILE_PREFIX ARCH_SYSTEMFILE_SFS ARCH_ZDRVSFS ARCH_BROWSER_VERSION ARCH_BROWSER_NAME
#
ARCH_CPU_FAMILY_1="fpu"
ARCH_CPU_EXTENSION_1="2mxtsc"
ARCH_CPU_FAMILY_2="mmx"
ARCH_CPU_EXTENSION_2="2mxtsc"
ARCH_CPU_FAMILY_3="sse"
ARCH_CPU_EXTENSION_3="2mxtsc"
ARCH_CPU_FAMILY_4="sse2"
ARCH_CPU_EXTENSION_4="2se2"
ARCH_CPU_FAMILY_5="pni|sse3"
ARCH_CPU_EXTENSION_5="2se3"
ARCH_CPU_FAMILY_6="ssse3"
ARCH_CPU_EXTENSION_6="3se3"
ARCH_CPU_FAMILY_7="sse4|sse4.x|sse4_2|sse4_1"
ARCH_CPU_EXTENSION_7="3se42"
ARCH_CPU_FAMILY_8="sse4a"
ARCH_CPU_EXTENSION_8="2se4a"
ARCH_CPU_FAMILY_9="avx"
ARCH_CPU_EXTENSION_9="avx"
ARCH_CPU_FAMILY_10="avx2|bmi|bmi2"
ARCH_CPU_EXTENSION_10="avx"
ARCH_CPU_FAMILY_11=""
ARCH_CPU_EXTENSION_11=""
export ARCH_CPU_FAMILY_1 ARCH_CPU_FAMILY_2 ARCH_CPU_FAMILY_3 ARCH_CPU_FAMILY_4 ARCH_CPU_FAMILY_5 ARCH_CPU_FAMILY_6 ARCH_CPU_FAMILY_7 ARCH_CPU_FAMILY_8 ARCH_CPU_FAMILY_9 ARCH_CPU_FAMILY_10 ARCH_CPU_FAMILY_11 
export ARCH_CPU_EXTENSION_1 ARCH_CPU_EXTENSION_2  ARCH_CPU_EXTENSION_3 ARCH_CPU_EXTENSION_4 ARCH_CPU_EXTENSION_5  ARCH_CPU_EXTENSION_6 ARCH_CPU_EXTENSION_7 ARCH_CPU_EXTENSION_8  ARCH_CPU_EXTENSION_9  ARCH_CPU_EXTENSION_10  ARCH_CPU_EXTENSION_11 
